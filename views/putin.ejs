	<body style="margin:0px;">
		<!--header-->
		<header class="navbar navbar-fixed-top navbar-inverse" role="navigation">
			<div class="container">
				<div class="navbar-header">
				  <a class="navbar-brand" href="/" style="color:#626262;">MOVE</a>
				</div>
				<div class="collapse navbar-col lapse">
				</div><!-- /.nav-collapse -->
			</div><!-- /.container -->
		</header><!-- /.navbar -->
		
		<!--main-->
		<div class="main">
            <!--form-->
            <form action="" method="POST">
                <div class="select">
                    <h2>실 신청하기</h2>
                    <select name="person" id="option">
                        <option value="신청인원선택">신청인원선택</option>
                        <option value="1">1</option>
                        <option value="2">2</option>
						<option value="3">3</option>
                        <option value="4">4</option>
                        <option value="5">5</option>
						<option value="6">6</option>
						<option value="7">7</option>
                        <option value="8">8</option>
                        <option value="9">9</option>
						<option value="10">10</option>
                    </select>
					<a id="text">명</a><br>
					
					<select name="grade" id="option">
						<option value="">학년선택</option>
						<option value="1">1</option>
						<option value="2">2</option>
						<option value="3">3</option>
					</select>
					<a id="text">학년</a>
					<select name="cla" id="option">
						<option value="">반선택</option>
						<option value="1">1</option>
						<option value="2">2</option>
						<option value="3">3</option>
						<option value="4">4</option>
					</select>
					<a id="text">반</a>
					<a id="text">번호</a>
					<select name="num" id="option">
						<option value="">번호선택</option>
						<option value="1">1</option>
						<option value="2">2</option>
						<option value="3">3</option>
						<option value="4">4</option>
						<option value="5">5</option>
						<option value="6">6</option>
						<option value="7">7</option>
						<option value="8">8</option>
						<option value="9">9</option>
						<option value="10">10</option>
						<option value="11">11</option>
						<option value="12">12</option>
						<option value="13">13</option>
						<option value="14">14</option>
						<option value="15">15</option>
						<option value="16">16</option>
						<option value="17">17</option>
						<option value="18">18</option>
						<option value="19">19</option>
						<option value="20">20</option>
                    </select>
                    <a id="text">이름</a>
					<input type="text" name="name" class="input" required autofocus>

					<input type="button" value="추가" onclick="add_textbox();" />

					<div id="t_space"></div>



                </div>
                <div class="select2">
                    <select name="room" id="option">
                        <option value="" selected=selected>실선택</option>
                        <option value="1층 응용프로그램 개발1실">1층 응용프로그램 개발1실</option>
                        <option value="1층 응용프로그램 개발2실">1층 응용프로그램 개발2실</option>
                        <option value="2층 홈베이스">2층 홈베이스</option>
                        <option value="2층 네트워크프로그래밍실습실">2층 네트워크프로그래밍실습실</option>
                        <option value="2층 시스템프로그래밍실습실">2층 시스템프로그래밍실습실</option>
                        <option value="3층 홈베이스">3층 홈베이스</option>
                        <option value="3층 마이크로프로세서1실">3층 마이크로프로세서1실</option>
                        <option value="3층 마이크로프로세서2실">3층 마이크로프로세서2실</option>
                        <option value="3층 프로그래밍실습2실">3층 프로그래밍실습2실</option>
                        <option value="4층 홈베이스">4층 홈베이스</option>
                        <option value="4층 프로그래밍실습1실">4층 프로그래밍실습1실</option>
                    </select>
                    <a id="text">오후</a>
                    <select name="s_hour" id="option">
                        <option value="7">7</option>
                        <option value="8">8</option>
                        <option value="9">9</option>
                    </select>
                    <a id="text">시</a>
                    <select name="s_min" id="option">
                        <option value="00">00</option>
                        <option value="10">10</option>
                        <option value="20">20</option>
                        <option value="30">30</option>
                        <option value="40">40</option>
                        <option value="50">50</option>
                    </select>
                    <a id="text">분  ~   </a>
                    <select name="e_hour" id="option">
                        <option value="7">7</option>
                        <option value="8">8</option>
                        <option value="9">9</option>
                    </select>
                    <a id="text">시</a>
                    <select name="e_min" id="option">
                        <option value="00">00</option>
                        <option value="10">10</option>
                        <option value="20">20</option>
                        <option value="30">30</option>
                        <option value="40">40</option>
                        <option value="50">50</option>
                    </select>
                    <a id="text">분</a><br>
                    
                    <a id="text" style="margin-top:20px;">신청사유</a><br>
					<input type="text" name="reason" class="reason" required autofocus>
					<%= message %>
                    <button class="btn" type="submit">신청하기</button>
                </div>
            </form>
			<!--form-->
				
		</div><!--/.container-->
		
		
